var list nil;

class foo {
  attr int hola;

  method void foo(int h) {
    this.hola = h;
  }

  method void bar() {
    print(0);
  }
}

class list extends foo {
  attr int x;
  attr list s;

  method void list(int x, list s) {
    this.hola = 0;
    this.x = x;
    this.s = s;
  }

  method int sum() {
    if (this === nil) {
      return 0;
    } else {
      var list s = this.s;
      return this.x + s.sum();
    }
  }
}

main {
  var const list test = new list(1, new list(2, nil));
  print(test.hola);
  print(test.sum());
}

