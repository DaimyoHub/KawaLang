var const list nil;

class list {
  attr int  x;
  attr list s;

  method void list(int x, list s) {
    this.x = x;
    this.s = s;
  }

  method int sum() {
    if (this === nil) {
      return 0;
    } else {
      var list s = this.s;
      return this.x + s.sum();
    }
  }
}

main {
  var list test =
    new list(1,
      new list(2,
        new list(3, nil)
      )
    );

  print(test.sum());
}

